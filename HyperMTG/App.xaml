                <ResourceDictionary Source="Resources/MagicStyles/SetSymbolGeometries.xaml" />
                <ResourceDictionary Source="Resources/MagicStyles/SetSymbolStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
