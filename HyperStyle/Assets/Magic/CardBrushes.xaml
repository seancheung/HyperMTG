    <ImageBrush x:Key="LPatternBrush"
                ImageSource="../../Resources/Pattern/land.jpg"
                TileMode="FlipXY" Viewport="0,0,0.5,0.5" />

    <LinearGradientBrush x:Key="OpacityLeftBrush" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Offset="0" Color="Black" />
        <GradientStop Offset="1" Color="Transparent" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="OpacityRightBrush" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Offset="0" Color="Transparent" />
        <GradientStop Offset="1" Color="Black" />
    </LinearGradientBrush>

    <VisualBrush x:Key="BRPatternBrush">
        <VisualBrush.Visual>
            <Grid>
                <Rectangle Width="740" Height="1068"
                           Fill="{StaticResource BPatternBrush}"
                           OpacityMask="{StaticResource OpacityLeftBrush}" />
                <Rectangle Width="740" Height="1068"
                           Fill="{StaticResource RPatternBrush}"
                           OpacityMask="{StaticResource OpacityRightBrush}" />
            </Grid>

        </VisualBrush.Visual>
    </VisualBrush>
    <VisualBrush x:Key="GUPatternBrush">
