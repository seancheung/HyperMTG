    <SolidColorBrush x:Key="RBarBrush" Color="#E2B88B" />
    <SolidColorBrush x:Key="WBarBrush" Color="#F7F9F3" />
    <SolidColorBrush x:Key="GBarBrush" Color="#C1C4AA" />
    <SolidColorBrush x:Key="ABarBrush" Color="#ABB1AB" />
    <SolidColorBrush x:Key="MBarBrush" Color="#C6B98D" />
    <SolidColorBrush x:Key="BBarBrush" Color="#B3AAA2" />
    <SolidColorBrush x:Key="CBarBrush" Color="#C4B8AF" />
    <SolidColorBrush x:Key="UBarBrush" Color="#C7D0D9" />

