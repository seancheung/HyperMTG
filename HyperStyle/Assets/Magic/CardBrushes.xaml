<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ImageBrush x:Key="MPatternBrush"
                ImageSource="../../Resources/Pattern/multicolor.jpg"
                TileMode="FlipXY" Viewport="0,0,0.5,0.5" />
    <ImageBrush x:Key="BPatternBrush"
                ImageSource="../../Resources/Pattern/black.jpg"
                TileMode="FlipXY" Viewport="0,0,0.5,0.5" />
    <ImageBrush x:Key="RPatternBrush"
                ImageSource="../../Resources/Pattern/red.jpg"
                TileMode="FlipXY" Viewport="0,0,0.5,0.5" />
    <ImageBrush x:Key="UPatternBrush"
                ImageSource="../../Resources/Pattern/blue.jpg"
                TileMode="FlipXY" Viewport="0,0,0.5,0.5" />
    <ImageBrush x:Key="GPatternBrush"
                ImageSource="../../Resources/Pattern/green.jpg"
                TileMode="FlipXY" Viewport="0,0,0.5,0.5" />
    <ImageBrush x:Key="WPatternBrush"
                ImageSource="../../Resources/Pattern/white.jpg"
                TileMode="FlipXY" Viewport="0,0,0.5,0.5" />
    <ImageBrush x:Key="APatternBrush"
                ImageSource="../../Resources/Pattern/artifact.jpg"
